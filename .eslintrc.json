{
  "env": {
    "browser": true,
    "es2021": true,
    "react-native/react-native": true
  },
  "settings": {
    "react": { "version": "detect" }
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:json/recommended",
    "plugin:prettier/recommended"
  ],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": ["react", "react-native"],
  "rules": {
    "react/prop-types": "off",
    "react/react-in-jsx-scope": "off"
  }
}
